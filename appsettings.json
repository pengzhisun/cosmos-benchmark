{
  "cosmosSettings": {
    "endpointUri": "https://{cosmos-db-account}.documents.azure.cn:443/",
    "primaryKey": "{cosmos-db-primary-key}",
    "database": "FinancialDatabase",
    "degreeOfParallelism": -1,
    "numberOfDocumentsToInsert": 50000
  },
  "collectionSettings": [
    {
      "dataFilePath": "{transactions-decompressed.json file path, e.g. C:\\Users\\pensun\\Downloads\\transactions-decompressed.json}",
      "id": "TransactionsCollection",
      "throughput": 10000,
      "partitionKeys": [
        "/costCenter"
      ],
      "uniqueKeys": []
    }
  ],
  "connectionPolicy": {
    "connectionMode": 1,
    "connectionProtocol": 1,
    "requestTimeout": "01:00:00",
    "maxConnectionLimit": 1000,
    "retryOptions": {
      "maxRetryAttemptsOnThrottledRequests": 10,
      "maxRetryWaitTimeInSeconds": 60
    }
  }
}